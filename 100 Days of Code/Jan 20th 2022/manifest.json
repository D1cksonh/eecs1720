{
  "manifest_version": 2,
  "name": "Tsung Extension Popup",
  "version": "2.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>" 
      ],
      "js": ["content.js"]
    }
  ],

  "browser_action": {
    "default_icon": "pencil.png",
    "default_popup": "sketch/popupTesting.html",
    "default_title": "A popup will come here."
  }
}